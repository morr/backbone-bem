<div class="b-round-navigation" id="navigation">
  <ul class="steps">
    <% for key,step of @steps: %>
      <li class="step" data-key="<%=key %>">
        <span class="index"><%=step.index+1 %></span>
        <span class="text"><%=step.name %></span>
        <span class="arrow"></span>
      </li>
    <% end %>
  </ul>
</div>

<div class="g-clear" id="wizard"></div>
